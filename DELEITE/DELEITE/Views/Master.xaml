<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DELEITE.Views.Master"
             Title="Master">
    <ContentPage.Content>
        
        <StackLayout   >
            <Button Text="Menu" x:Name="Menu" VerticalOptions="Center" CornerRadius="50"   BackgroundColor="Aquamarine" BorderColor="Black" BorderWidth="1.5"  Clicked="Menu_Clicked" ></Button>

            <Button Text="Buy"  CornerRadius="50" x:Name="buy"  BackgroundColor="Aquamarine" BorderColor="Black" BorderWidth="1.5" Clicked="buy_Clicked" ></Button>
            <Button Text="Bills" CornerRadius="50" x:Name="bills"   BackgroundColor="Aquamarine" BorderColor="Black" BorderWidth="1.5"  Clicked="bills_Clicked" ></Button>
            <Button Text="Offers" CornerRadius="50" x:Name="offers"   BackgroundColor="Aquamarine" BorderColor="Black" BorderWidth="1.5"   Clicked="offers_Clicked" ></Button>
            <Button Text="us " CornerRadius="50" x:Name="us"    BackgroundColor="Aquamarine" BorderColor="Black" BorderWidth="1.5"   Clicked="us_Clicked" ></Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>